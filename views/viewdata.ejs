<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>

   
    <table style="border: 1px solid;">
        <tr>
            <th>Email</th>
            <th>Mobile</th>
            <th>fname</th>
            <th>Delete</th>
            <th>Edit</th>
           
        </tr>
        <% for(var i=0;i<result.length;i++) {%> 
        <tr>
            <td><%= result[i].email %></td>
            <td><%= result[i].mobile %></td>
            <td><%= result[i].fname %></td> 
           <td><a href="/delete/<%=result[i]._id %>">Delete</a></td>
           <td><a href="/edit/<%=result[i]._id %>">Edit</a></td>
        </tr> 
    
        <%} %>
    </table>
</body>
</html>